{
  "name": "omakase-ai",
  "version": "0.1.0",
  "description": "AI-powered conversational commerce platform with Voice & Chat agents",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/server/index.ts",
    "dev:lib": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/server/index.js",
    "server": "tsx src/server/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:mcp": "vitest run tests/mcp-servers",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "ai",
    "voice",
    "chat",
    "conversational-commerce",
    "agents"
  ],
  "author": "shunsuke",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.12",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "multer": "^2.0.2",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@openai/agents": "^0.3.4",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "openai": "^6.10.0",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  }
}
